{
  "name": "rise-player-electron",
  "version": "1.0.0",
  "description": "Electron Player",
  "main": "main.js",
  "author": "Rise Vision",
  "license": "GPLv3",
  "devDependencies": {
    "electron-ipc-mock": "^0.0.2",
    "istanbul": "^0.4.0",
    "jshint": "^2.9.5",
    "mocha": "^3.4.2",
    "mocha-junit-reporter": "^1.9.1",
    "simple-mock": "^0.6.0"
  },
  "dependencies": {
    "angular": "1.5.9",
    "camelcase": "^2.1.0",
    "common-display-module": "git://github.com/Rise-Vision/common-display-module.git#chore/initial-files",
    "electron": "1.6.6",
    "electron-mocha": "^4.0.0",
    "electron-packager": "8.6.0",
    "electron-proxy-agent": "git://github.com/Rise-Vision/node-electron-proxy-agent.git#8d66",
    "gunzip-maybe": "^1.2.1",
    "marked": "~0.3.5",
    "mkdirp": "^0.5.1",
    "ncp": "git://github.com/Rise-Vision/ncp.git",
    "node-fetch": "1.3.3",
    "object.values": "^1.0.4",
    "primus": "^6.0.1",
    "request": "2.73.0",
    "rimraf": "2.4.3",
    "rise-common-electron": "git://github.com/Rise-Vision/rise-common-electron.git#v2.1.2",
    "rise-cache-v2": "git://github.com/Rise-Vision/rise-cache-v2.git#4a60fa0",
    "tar-fs": "^1.8.1",
    "windows-shortcuts": "git://github.com/Rise-Vision/windows-shortcuts.git",
    "ws": "^1.1.1",
    "yargs": "~3.28.0"
  },
  "scripts": {
    "test": "jshint . && istanbul cover node_modules/mocha/bin/_mocha -r ./init.js -- --reporter mocha-junit-reporter --reporter-options mochaFile=test/test-results.xml --recursive test/unit",
    "test-no-coverage": "jshint . && ./node_modules/mocha/bin/mocha -r ./init.js --recursive test/unit",
    "integration": "export ELECTRON_PATH=$PWD/node_modules/.bin/electron && node_modules/.bin/electron-mocha -r ./init.js -t 10000 test/integration",
    "ci-integration": "xvfb-run node_modules/.bin/electron-mocha -r init.js -t 10000 test/integration",
    "ui-integration": "export ELECTRON_PATH=$PWD/node_modules/.bin/electron && node_modules/.bin/electron-mocha -r init.js test/integration/ui/main-it.js",
    "ci-ui-integration": "xvfb-run node_modules/.bin/electron-mocha -r init.js test/integration/ui/main-it.js",
    "start": "./node_modules/.bin/electron main.js"
  }
}
